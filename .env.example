# Основные параметры приложения
DEV_MODE=true
LOG_LEVEL=INFO
ALLOWED_HOSTS=*

# PostgreSQL
DATABASE_DSN=postgresql+asyncpg://postgres:postgres@localhost:5432/georag
DATABASE_ECHO=false
# DATABASE_DSN_DOCKER=postgresql+asyncpg://postgres:postgres@postgres:5432/georag

# Qdrant (векторное хранилище)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=georag_images
QDRANT_SHARDS=1
QDRANT_REPLICATION=1
QDRANT_ON_DISK=true

# S3-совместимое объектное хранилище
S3_BUCKET=georag
S3_REGION=us-east-1
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_SESSION_TOKEN=
S3_PUBLIC_BASE_URL=http://localhost:9000/georag
S3_PRESIGN_TTL=3600
S3_MAX_PARALLEL=16
STORAGE_IMAGE_SUBDIR=images
STORAGE_FEATURE_SUBDIR=features
STORAGE_PREVIEW_SUBDIR=previews
STORAGE_QUERY_SUBDIR=queries

# Настройки извлечения и кеширования признаков
FEATURE_MAX_KEYPOINTS=4096
LOCAL_FEATURE_TYPE=superpoint_max
GLOBAL_DESCRIPTOR_TYPE=netvlad
MATCHER_TYPE=lightglue
RETRIEVAL_CANDIDATES=32
GLOBAL_SCORE_WEIGHT=0.35
LOCAL_SCORE_WEIGHT=0.65
GEOMETRY_SCORE_WEIGHT=0.25
MAX_SEARCH_RESULTS=50
FEATURE_CACHE_SIZE_MB=1536
FEATURE_CACHE_ITEMS=512
FEATURE_PREFETCH_LIMIT=16
POINT_CLOUD_LIMIT=2048
COMPUTE_DEVICE=

# Геокодирование Nominatim
NOMINATIM_USER_AGENT=GeoRAG/1.0 (+https://example.com)
NOMINATIM_TIMEOUT=5

# Источники данных
MAPILLARY_TOKEN=your_mapillary_access_token
